valgrind --leak-check=full --leak-check=yes --show-leak-kinds=all --track-fds=yes --trace-children=yes --show-reachable=yes --suppressions=readline.supp ./minishell





valgrind \
  --leak-check=full \
  --show-leak-kinds=all \
  --track-fds=yes \
  --trace-children=no \
  --show-reachable=yes \
  --suppressions=readline.supp \
  ./minishell
